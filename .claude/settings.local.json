{
  "permissions": {
    "allow": [
      "Bash(test:*)",
      "Bash(command -v:*)",
      "Bash(shellcheck:*)",
      "Bash(./install.sh:*)",
      "Bash(command grep \"DRY RUN\\|SUCCESS\\|Backed\")",
      "Bash(gh auth status:*)",
      "Bash(git config:*)",
      "Bash(gh --version:*)",
      "Bash(mkdir:*)",
      "Bash(mv:*)",
      "Bash(cat:*)",
      "Bash(source:*)",
      "Bash(echo:*)",
      "Bash(gpgconf:*)",
      "Bash(tty:*)",
      "Bash(symlink_gpg_config)",
      "Bash(bash -c 'source install/common.sh && DOTFILES_DIR=\"\"$(pwd)\"\" && source install/symlinks.sh && symlink_gpg_config')",
      "Bash(~/.gnupg/gpg-agent.conf)",
      "Bash(chmod:*)",
      "Bash(zsh -c 'source ~/.config/dotfiles/shell/env && echo \"\"GPG_TTY after source: $GPG_TTY\"\"')",
      "Bash(zsh:*)",
      "Bash(bash -c:*)",
      "Bash(bash:*)",
      "Bash(mise ls-remote:*)",
      "Bash(/usr/bin/grep -E \"Font|Cursor|Foreground|Background|Columns|Rows\" /tmp/iterm2_settings.xml)",
      "Bash(/usr/bin/grep -A 2 \"Normal Font\" /tmp/iterm2_settings.xml)",
      "Bash(/usr/bin/grep -B 5 -A 20 \"New Bookmarks\" /tmp/iterm2_settings.xml)",
      "Bash(find:*)",
      "Bash(/usr/bin/grep -v \"no matches found\")",
      "Bash(for script in install.sh install/*.sh shell/functions.d/*.sh)",
      "Bash(do echo \"=== Checking: $script ===\" shellcheck \"$script\")",
      "Bash(/usr/bin/find:*)",
      "Bash(/usr/bin/grep -B1 \"sh$\")",
      "Bash(lefthook version:*)",
      "Bash(lefthook install:*)",
      "Bash(/usr/bin/grep:*)",
      "Bash(eval '$(mise activate bash)')",
      "Bash(lefthook --version:*)"
    ],
    "deny": [],
    "ask": []
  }
}
