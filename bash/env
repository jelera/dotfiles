# export PATH=/usr/bin/:/usr/local:/usr/local/bin:/bin:/usr/local/sbin:/usr/sbin:/sbin:$HOME/.bin:$PATH
export PATH=/usr/local/bin/:/usr/local:/usr/bin:/bin:/usr/local/sbin:/usr/sbin:/sbin:$HOME/.bin:$PATH

# for tmux: export 256color
export TERM=xterm-256color
[ -n "$TMUX" ] && export TERM=screen-256color
